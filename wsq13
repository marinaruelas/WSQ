#include <iostream>
using namespace std;

double square (int a){
  double b=a;
     while ((b-a/b)/2> 0.000001){
         b= (b+a/b)/2;
       }
  return b;
}
int main (){
  int a;
  cout << "Enter the number you want to know the square root" <<endl;
  cin >> a;
  cout << "The square root is: " << square (a) <<endl;
  return 0;
}
